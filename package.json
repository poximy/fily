{
	"name": "startup",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"next-lint": "next lint",
		"test-types": "yarn tsc",
		"test-unit": "yarn jest test/unit/",
		"testonly": "yarn jest --runInBand",
		"lint-eslint": "eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.json --no-eslintrc",
		"lint": "run-p test-types lint-typescript prettier-check lint-eslint lint-language",
		"lint-fix": "yarn prettier-fix && eslint . --ext js,jsx,ts,tsx --fix --max-warnings=0 --config .eslintrc.json --no-eslintrc",
		"prettier-check": "prettier --check .",
		"prettier-fix": "prettier --write ."
	},
	"dependencies": {
		"@chakra-ui/react": "^1.8.3",
		"@emotion/react": "^11.7.1",
		"@emotion/styled": "^11.6.0",
		"@prisma/client": "^3.10.0",
		"framer-motion": "^6.2.6",
		"is-mobile": "^3.1.0",
		"next": "12.0.10",
		"next-auth": "^4.2.1",
		"react": "17.0.2",
		"react-dom": "17.0.2"
	},
	"devDependencies": {
		"@faker-js/faker": "^6.0.0-alpha.7",
		"@testing-library/jest-dom": "^5.16.2",
		"@testing-library/react": "^12.1.3",
		"@types/jest": "^27.4.1",
		"@types/node": "^17.0.21",
		"@types/react": "17.0.39",
		"eslint": "8.9.0",
		"eslint-config-next": "12.0.10",
		"jest": "^27.5.1",
		"postcss": "^8.4.6",
		"prettier": "^2.5.1",
		"prettier-plugin-prisma": "^3.10.0",
		"prisma": "^3.10.0",
		"ts-node": "^10.6.0",
		"typescript": "4.5.5"
	}
}
